            <snapshots>
                <enabled>true</enabled>
                <updatePolicy>always</updatePolicy>
            </snapshots>
        </repository>
        
        <repository>
            <id>jboss.org-nexus-public-repository</id>
            <url>https://repository.jboss.org/nexus/content/groups/public-jboss/</url>
            <releases>
                <enabled>true</enabled>
                <updatePolicy>always</updatePolicy>
            </releases>
            <snapshots>
                <enabled>true</enabled>
                <updatePolicy>always</updatePolicy>
            </snapshots>
        </repository>
        
        <!-- ForgeRock.org Project Repository -->
        <repository>
          <id>forgerock-central</id>
          <name>repo</name>
          <url>http://maven.forgerock.org/repo/repo</url>
