    <!-- Component Definition -->
    <name>openam</name>
    <groupId>org.forgerock</groupId>
    <artifactId>openam</artifactId>
    <version>10.1.0-SNAPSHOT</version>
    <packaging>pom</packaging>

    <description>OpenAM</description>
    <inceptionYear>2010</inceptionYear> <!-- Need correct Year -->
    <organization>
        <name>ForgeRock</name>
        <url>http://www.forgerock.org</url>
    </organization>
    <url>http://www.forgerock.org</url>

    <!-- Component Properties -->
    <properties>
        <!-- **************************************************************** -->
        <maven.build.timestamp.format>yyyy-MM-dd HH:mm:ss</maven.build.timestamp.format>
        <!-- **************************************************************** -->
        <!-- Java Properties -->
        <java.source.version>1.6</java.source.version>
        <java.target.version>1.6</java.target.version>
        <!-- Surefire Argument Line -->
        <java.surefire.options>-Xms2g -Xmx2g -XX:MaxPermSize=512m</java.surefire.options>
        <!-- Project Encoding -->
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>

    <!-- Child Modules -->
    <modules>
        <!-- OpenAM New Modules -->
        <module>openam-shared</module>
        <module>openam-schema</module>
        <module>openam-core</module>
        <module>openam-federation</module>
        <module>openam-server</module>
        <module>openam-extlib</module>
        <!-- OpenAM Build out extlib for subsequent Legacy antRun -->
        <!-- OpenAM Legacy Ant Wrapper -->
        <module>openam</module>
        <!-- Final Packaging Artifact -->
        <module>openam-distribution</module>
    </modules>

