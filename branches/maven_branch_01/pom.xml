        </pluginRepository>
        <pluginRepository>
          <snapshots />
          <id>snapshots</id>
          <name>repo</name>
          <url>http://maven.forgerock.org/repo/repo</url>
        </pluginRepository>
     
    </pluginRepositories>
    
